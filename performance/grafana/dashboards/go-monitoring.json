{
  "id": 1,
  "title": "Go Application Monitoring",
  "tags": ["go", "application", "monitoring"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Memory Usage (MB)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "go_memstats_alloc_bytes{job=\"go-app\"} / 1024 / 1024",
          "legendFormat": "Allocated Memory (MB)",
          "refId": "A"
        },
        {
          "expr": "go_memstats_sys_bytes{job=\"go-app\"} / 1024 / 1024",
          "legendFormat": "System Memory (MB)",
          "refId": "B"
        },
        {
          "expr": "go_memstats_heap_alloc_bytes{job=\"go-app\"} / 1024 / 1024",
          "legendFormat": "Heap Allocated (MB)",
          "refId": "C"
        },
        {
          "expr": "go_memstats_heap_sys_bytes{job=\"go-app\"} / 1024 / 1024",
          "legendFormat": "Heap System (MB)",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "CPU Usage",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(process_cpu_seconds_total{job=\"go-app\"}[5m]) * 100",
          "legendFormat": "CPU Usage %",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Garbage Collection Duration (ms)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(go_gc_duration_seconds_sum{job=\"go-app\"}[5m]) * 1000",
          "legendFormat": "GC Duration (avg)",
          "refId": "A"
        },
        {
          "expr": "go_gc_duration_seconds{job=\"go-app\", quantile=\"0.5\"} * 1000",
          "legendFormat": "GC Duration (50th percentile)",
          "refId": "B"
        },
        {
          "expr": "go_gc_duration_seconds{job=\"go-app\", quantile=\"0.95\"} * 1000",
          "legendFormat": "GC Duration (95th percentile)",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "title": "Garbage Collection Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(go_gc_duration_seconds_count{job=\"go-app\"}[5m])",
          "legendFormat": "GC Rate (collections/sec)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    },
    {
      "id": 5,
      "title": "Goroutines",
      "type": "timeseries",
      "targets": [
        {
          "expr": "go_goroutines{job=\"go-app\"}",
          "legendFormat": "Goroutines",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 16
      }
    },
    {
      "id": 6,
      "title": "Heap Objects (Count)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "go_memstats_heap_objects{job=\"go-app\"}",
          "legendFormat": "Heap Objects",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 16
      }
    },
    {
      "id": 7,
      "title": "Memory Allocation Rate (MB/s)",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(go_memstats_alloc_bytes_total{job=\"go-app\"}[5m]) / 1024 / 1024",
          "legendFormat": "Allocation Rate (MB/s)",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "MBs",
          "min": 0
        }
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 24
      }
    },
    {
      "id": 8,
      "title": "Memory Stats Summary",
      "type": "stat",
      "targets": [
        {
          "expr": "go_memstats_alloc_bytes{job=\"go-app\"} / 1024 / 1024",
          "legendFormat": "Current Alloc (MB)",
          "refId": "A"
        },
        {
          "expr": "go_memstats_sys_bytes{job=\"go-app\"} / 1024 / 1024",
          "legendFormat": "System Memory (MB)",
          "refId": "B"
        },
        {
          "expr": "go_goroutines{job=\"go-app\"}",
          "legendFormat": "Goroutines",
          "refId": "C"
        },
        {
          "expr": "go_memstats_heap_objects{job=\"go-app\"}",
          "legendFormat": "Heap Objects",
          "refId": "D"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Current Alloc (MB)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "decmbytes"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "System Memory (MB)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "decmbytes"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  },
  "refresh": "5s",
  "schemaVersion": 27,
  "version": 0,
  "links": []
}
