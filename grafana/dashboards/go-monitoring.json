        },
        {
          "expr": "go_memstats_heap_objects{job=\"chat-app\"}",
          "legendFormat": "Heap Objects",
          "refId": "D"
        }
      ],
      "unit": "short",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 24
      }
    }
  ],
