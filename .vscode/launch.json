{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Kafka MUTEX - Producer",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/kafka/cmd/benchmark",
      "env": {
        "CG_ID": "producer-group",
        "SHOULD_PRODUCE": "false",
        "APP_ENV": "debug"
      }
    },
    {
      "name": "Debug Kafka Chans - Producer",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/kafka-chans/cmd/benchmark",
      "env": {
        "CG_ID": "producer-group",
        "SHOULD_PRODUCE": "true",
        "APP_ENV": "debug"
      }
    },
    {
      "name": "Debug Kafka Outbox",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/kafka-outbox/cmd/server",
      "env": {
        "CG_ID": "producer-group",
        "SHOULD_PRODUCE": "true",
        "APP_ENV": "debug"
      }
    }
  ]
}
